<h1>Destinations</h1>

<table>
  <thead>
    <tr>
      <th>Destination</th>
      <th>Country</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @destinations.each do |destination| %>
      <tr>
        <td><%= link_to destination.name, destination_path(destination) %></td>
        <td><%= destination.country%></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
